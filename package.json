{
  "bugs": "https://github.com/darbymanning/qudini/issues",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "1.7.4",
    "node-fetch": "^2.6.1",
    "polka": "0.5.2",
    "sirv": "1.0.10"
  },
  "description": "Custom front-end for Qudini Kiosk",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "@neuekit/reboot": "1.10.2",
    "@neuekit/utilities": "1.7.1",
    "@rollup/plugin-alias": "3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-url": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "autoprefixer": "^10.1.0",
    "babel-jest": "^26.6.3",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "js-cookie": "^2.2.1",
    "libphonenumber-js": "^1.9.9",
    "npm-run-all": "4.1.5",
    "onchange": "7.1.0",
    "postcss": "8.2.1",
    "postcss-cli": "8.3.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.1",
    "rollup": "^2.35.1",
    "rollup-jest": "^1.1.1",
    "rollup-plugin-svelte": "7.0.0",
    "rollup-plugin-terser": "7.0.2",
    "sapper": "0.28.10",
    "sass": "^1.30.0",
    "stylelint": "^13.9.0",
    "stylelint-order": "^4.1.0",
    "svelte": "^3.31.0",
    "svelte-jester": "^1.3.0",
    "svelte-preprocess": "^4.6.1",
    "yarn": "^1.22.10"
  },
  "homepage": "https://qudini.app",
  "license": "MIT",
  "name": "qudini",
  "scripts": {
    "autoprefixer": "postcss -u autoprefixer -r ./static/global.css --no-map",
    "build": "run-s 'build:css' 'prettier:check' 'stylelint:check' 'test' && sapper build --legacy",
    "build:css": "run-s 'stylelint:write' 'scss' 'autoprefixer' 'reload'",
    "dev": "yarn build:css && run-p 'sapper' 'watch:css'",
    "export": "sapper export --legacy",
    "prettier:check": "prettier --c --plugin-search-dir=. 'src/**/*.js' 'src/**/*.svelte'",
    "prettier:write": "prettier --write --plugin-search-dir=. 'src/**/*.js' 'src/**/*.svelte'",
    "reload": "touch src/routes/_layout.svelte",
    "sapper": "sapper dev",
    "scss": "sass --no-source-map --load-path=node_modules -s compressed ./src/global.scss ./static/global.css",
    "start": "node __sapper__/build",
    "stylelint:check": "stylelint 'src/**/*.svelte' 'src/**/*.scss'",
    "stylelint:write": "stylelint 'src/**/*.svelte' 'src/**/*.scss' --fix",
    "test": "jest",
    "watch:css": "onchange './src/**/*.scss' -- yarn build:css"
  },
  "version": "0.1.0"
}
